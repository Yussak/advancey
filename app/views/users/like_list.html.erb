<% provide(:title, "いいね一覧") %>
<div class="row">
  <div class="col-md-8 mx-auto">
    <%= render 'users/user_profile' %>
  </div>
</div>
<hr>
<div class="row">
  <div class="col-md-8 mx-auto">
    <h3>いいね一覧</h3>
    <% if @likes.any? %>
    <ul class="feed_list">
      <li><%= render @likes %></li>
    </ul>
    <%= paginate @likes %>
    <% else %>
    <p>まだいいねしていません</p>
    <% end %>
  </div>
</div>